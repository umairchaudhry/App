<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.example.umair.application.MainActivity">

<Button
    android:id="@+id/LunchMapp"
    android:layout_width="400dp"
    android:layout_height="60dp"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:layout_marginTop="105dp"
    android:onClick="process"
    android:text="Lunch Mapp" />

<Button
    android:id="@+id/lunchMarket"
    android:layout_width="400dp"
    android:layout_height="wrap_content"
    android:text="lunch Market"
    android:layout_below="@+id/LunchMapp"
    android:layout_alignParentStart="true"
    android:onClick="process"
    android:layout_marginTop="15dp" />

<Button
    android:id="@+id/SendEmail"
    android:layout_width="400dp"
    android:layout_height="60dp"
    android:layout_marginTop="16dp"
    android:text="Send Email"
    android:layout_below="@+id/lunchMarket"
    android:onClick="process"
    android:layout_alignParentStart="true" />


</RelativeLayout>